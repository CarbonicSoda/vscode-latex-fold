{
	"name": "latex-fold",
	"displayName": "LaTeX Fold",
	"description": "Folding Support for LaTeX Section Headers",
	"version": "0.0.1",
	"publisher": "carbonsoda",
	"author": {
		"name": "David W."
	},
	"homepage": "https://github.com/CarbonicSoda/vscode-latex-fold/blob/master/README.md",
	"repository": {
		"type": "git",
		"url": "https://github.com/CarbonicSoda/vscode-latex-fold"
	},
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/CarbonicSoda/vscode-latex-fold/issues",
		"email": "carbonsoda@outlook.com"
	},
	"engines": {
		"vscode": "^1.80.0"
	},
	"keywords": [
		"latex",
		"LaTeX",
		"fold",
		"section"
	],
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onLanguage:latex"
	],
	"icon": "media/icon.png",
	"main": "./dist/extension.js",
	"browser": "./dist/web/extension.js",
	"scripts": {
		"vscode:prepublish": "pnpm run package",
		"compile": "pnpm run check-types && pnpm run lint && node esbuild.js && node esbuild.web.js",
		"watch": "npm-run-all -p watch:*",
		"watch:esbuild": "node esbuild.js --watch && node esbuild.web.js --watch",
		"watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
		"package": "pnpm run check-types && pnpm run lint && node esbuild.js --production && node esbuild.web.js --production",
		"compile-tests": "tsc -p . --outDir out",
		"watch-tests": "tsc -p . -w --outDir out",
		"pretest": "pnpm run compile-tests && pnpm run compile && pnpm run lint",
		"check-types": "tsc --noEmit",
		"lint": "eslint src",
		"test": "vscode-test",
		"run-in-browser": "vscode-test-web --browserType=chromium --extensionDevelopmentPath=. ."
	},
	"devDependencies": {
		"@esbuild-plugins/node-globals-polyfill": "^0.2.3",
		"@types/mocha": "^10.0.10",
		"@types/node": "20.x",
		"@types/vscode": "^1.80.0",
		"@typescript-eslint/eslint-plugin": "^8.17.0",
		"@typescript-eslint/parser": "^8.17.0",
		"@vscode/test-cli": "^0.0.10",
		"@vscode/test-electron": "^2.4.1",
		"@vscode/test-web": "^0.0.65",
		"esbuild": "^0.24.0",
		"eslint": "^9.16.0",
		"npm-run-all": "^4.1.5",
		"typescript": "^5.7.2"
	}
}
